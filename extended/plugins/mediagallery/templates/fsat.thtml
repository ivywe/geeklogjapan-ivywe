{# begin {templatelocation} #}

<script type="text/javascript">
    var flashvars = {};
    var params = {
    wmode: "transparent"
    };
    var attributes = {};

    flashvars.action="true";

    flashvars.xmlUrl="{movie}&amp;showDropShadow={dropshadow}&amp;delay={delay}&amp;transInType=Fade&amp;transOutType=Fade&amp;noLink={nolink}&amp;showControls=true{showtitle}";
    swfobject.embedSWF("{site_url}/players/minislideshow.swf", "{id}", "{width}", "{height}", "9.0.115.0", "expressInstall.swf", flashvars, params, attributes);
</script>
<div id="{id}">
    <a href="http://www.adobe.com/go/getflashplayer">
        <img src="http://www.adobe.com/images/shared/download_buttons/get_flash_player.gif" alt="Get Adobe Flash player"{xhtml}>
    </a>
</div>

{# end {templatelocation} #}
